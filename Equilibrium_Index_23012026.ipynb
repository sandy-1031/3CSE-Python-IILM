    "# Finding the equilibrium index\n",
    "l = [1, 2, 2, 9, 3, 2]\n",
    "total_sum = sum(l)\n",
    "left_sum = 0\n",
    "for i in range(len(l)):\n",
    "    right_sum = total_sum - left_sum - l[i]\n",
    "\n",
    "    if left_sum == right_sum:\n",
    "        print(i) \n",
    "        break\n",
    "        \n",
    "    left_sum += l[i]\n",
    "    "
